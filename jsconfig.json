{
    "compilerOptions": {
        "jsx": "react",
        "baseUrl": ".",
        "paths": {
            "src/*": [
                "src/*"
            ],
            "public/*": [
                "public/*"
            ],
            "components/*": [
                "src/components/*"
            ],
            "hooks/*": [
                "src/hooks/*"
            ],
            "utils/*": [
                "src/utils/*"
            ],
        }
    },
    "exclude": [
        "node_modules"
    ]
}